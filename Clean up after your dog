function crap(x, bags, cap){

const tcap = bags * cap;
let count = 0;
let i,j;

for (i = 0; i < x.length; i+=1) {
  for (j  =0; j < x[i].length; j+=1){
    if (x[i][j] === '@') {
      count +=1 ;  
    } else {
      if (x[i][j] === 'D') return 'Dog!!'
    }
  }
}
return count <= tcap ? 'Clean' : 'Cr@p';
}
